package templates

import "jquery-demo/types"

templ SearchResults(customers []types.Customer) {
	<table>
		<tr>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Email</th>
		</tr>
		for _, customer := range customers {
			<tr>
				<td>{ customer.FirstName }</td>
				<td>{ customer.LastName }</td>
				<td>{ customer.Email }</td>
			</tr>
		}
	</table>
}
